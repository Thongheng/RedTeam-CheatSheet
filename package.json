{
  "name": "redsploit-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:extension": "tsc -b && vite build --mode extension",
    "build:extension:chrome": "npm run build:extension && cp public/manifest.json dist/",
    "build:extension:firefox": "npm run build:extension && cp public/manifest-firefox.json dist/manifest.json",
    "package:chrome": "npm run build:extension:chrome && cd dist && zip -r ../hacktoy-chrome.zip .",
    "package:firefox": "npm run build:extension:firefox && cd dist && zip -r ../hacktoy-firefox.zip .",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@codemirror/lang-sql": "^6.10.0",
    "@uiw/codemirror-theme-dracula": "^4.25.4",
    "@uiw/react-codemirror": "^4.25.4",
    "escape-quotes": "^1.0.2",
    "jose": "^6.1.3",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.562.0",
    "node-sql-parser": "^5.3.13",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}